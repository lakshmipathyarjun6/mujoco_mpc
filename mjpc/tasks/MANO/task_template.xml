<mujoco model="MANO General Task">

    <include file="../common.xml"/>

    <custom>
        <numeric name="agent_planner" data="0" />
        <numeric name="agent_horizon" data="2.5" />
        <numeric name="agent_timestep" data="0.01" />
        <numeric name="agent_policy_width" data="0.005" />
        <numeric name="sampling_trajectories" data="10" />
        <numeric name="sampling_spline_points" data="6" />
        <numeric name="default_sampling_exploration" data="0.1" />
        <numeric name="root_pos_sampling_exploration" data="0.1" />
        <numeric name="root_quat_sampling_exploration" data="0.01" />
        <numeric name="gradient_spline_points" data="6" />
        <numeric name="mocap_reference_framerate" data="12" />
        <numeric name="ball_motor_kp" data="10" />
        <numeric name="ball_motor_kd" data="0.1" />
    </custom>

    <sensor>
        <user name="Object Position" dim="3" user="0 1000.0 0 1000"/>
        <user name="Object Orientation" dim="3" user="0 10.0 0 20"/>
    </sensor>

    <include file="right_hand_template.xml"/>

</mujoco>
